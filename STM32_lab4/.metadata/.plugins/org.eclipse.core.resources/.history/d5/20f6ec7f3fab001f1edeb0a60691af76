void toggleRed1(){
	HAL_GPIO_TogglePin(RED1_GPIO_Port, RED1_Pin);
}
void toggleYellow1(){
	HAL_GPIO_TogglePin(YEL1_GPIO_Port, YEL1_Pin);
}
void toggleGreen1(){
